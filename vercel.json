{
    "builds": [
      {
        "src": "front/package.json",
        "use": "@vercel/static-build",
        "config": { 
            "command": "npm run build",
            "output": "front/dist"
         }
      },
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "src/api/$1" },
      { "src": "/(.*)", "dest": "frontend/$1" }
    ]
  }